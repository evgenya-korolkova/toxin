block variables
    - var title

doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title #{title}
        link(rel="icon" type="image/png" href=require("@/icons/icons.png"))
    body
        .ui-container
            img.ui-logo(src=require("@/img/logo.png") alt="logo")
            block content